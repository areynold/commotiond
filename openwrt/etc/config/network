config interface 'loopback'
        option ifname 'lo'
        option proto 'static'
        option ipaddr '127.0.0.1'
        option netmask '255.0.0.0'

config interface 'plug'
        option ifname 'eth0'
        option proto 'static'
        option ipaddr '102.126.247.1'
	option netmask '255.255.255.0'

